<h1>Package: <%= @package.name %></h1>

<table class="single_pack">
    <tr><th>Shipping Co.</th><td><%= @package.shipping_co %></td></tr>
    <tr><th>Delivery Date</th><td><%= @package.delivery_date %></td></tr>
    <tr><th>Status</th><td><%= @package.status %></td></tr>
    <tr><th>Tracking No</th><td><%= @package.tracking_no %></td></tr>
    <tr><th>Sender</th><td><%= @package.sender.name %></td></tr>
  </table>
  <br>
  <td><a href="/packages/<%= @package.id %>/edit">Edit</a></td>
  <td><a href="/packages/<%= @package.id %>/delete">Delete</a></td>
